<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	<PublishSelfContained>true</PublishSelfContained>
	<SelfContained>true</SelfContained>
	<RuntimeIdentifier>win-x64</RuntimeIdentifier>
  </PropertyGroup>
  
  <ItemGroup>
	<Protobuf Include="Protos\notification.proto" GrpcServices="Server" />
	<Protobuf Include="Protos\task.proto" GrpcServices="Server" />
	<Protobuf Include="Protos\telegram.proto" GrpcServices="Server" />
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="DeskAssistant.Core" Version="1.3.0" />
    <PackageReference Include="Grpc.AspNetCore" Version="2.71.0" />
	<PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.10" />
	<PackageReference Include="Microsoft.Extensions.Hosting.WindowsServices" Version="9.0.10" />
	<PackageReference Include="NLog" Version="6.0.6" />
	<PackageReference Include="NLog.Web.AspNetCore" Version="6.0.6" />
	<PackageReference Include="Npgsql.EntityFrameworkCore.PostgreSQL" Version="9.0.4" />
	<PackageReference Include="Telegram.Bot" Version="22.7.5" />
  </ItemGroup>

</Project>
